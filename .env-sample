
PHP_VER=8.0
MYSQL_VER=8.0
MYSQL_ROOT_PASSWORD=password
MYSQL_PASSWORD=password
MYSQL_DATABASE=wordpress
MYSQL_USER=user
SQL_DUMP_DATA=/sql/wordpress.sql.gz
WP_VERSION=5.8.1
WP_URL=localhost
WP_ROOT=/var/www/html
WP_DB_PREFIX=wp_
WP_ADMIN_USER=admin
WP_ADMIN_PASSWORD=password
WP_ADMIN_EMAIL=example@example.com
WP_INSTALL_PLUGINS=query-monitor wordpress-seo mw-wp-form webp-converter-for-media all-in-one-wp-migration google-sitemap-generator post-types-order taxonomy-terms-order post-duplicator
WP_ACTIVATE_PLUGINS=query-monitor wordpress-seo webp-converter-for-media google-sitemap-generator
WP_THEME_NAME=wp-exercise
# Replace following 'wp-exercise' to your theme name
MIX_SRC_RELATIVE_PATH=resources/themes/wp-exercise
MIX_DIST_RELATIVE_PATH=wp/wp-content/themes/wp-exercise
MIX_BROWSER_SYNC_HOST=localhost
MIX_BROWSER_SYNC_PORT=3000
# URL of WordPress running on your local environment
# Attention: Do not set '.local' as domain. Because it makes BrowserSync slow
MIX_BROWSER_SYNC_PROXY=http://localhost
# If protocol of localhost is https, following fields are required
# MIX_BROWSER_SYNC_HTTPS_KEY=/Users/input-user-name/.ssl/localhost.key
# MIX_BROWSER_SYNC_HTTPS_CERT=/Users/input-user-name/.ssl/localhost.crt
